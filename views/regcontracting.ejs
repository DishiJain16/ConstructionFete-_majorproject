		<script type="text/javascript">
		
		function validate1()
		{
		
		var email1 = document.getElementById("Email1").value;
		var regx02 = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		if(regx02.test(email1))
		{

		console.log("true");		
		}
		else{
		alert("Please enter valid E-mail");
		return false;
		}
		
		var mo1 = document.getElementById("mob1").value;
		var regx01 = /^[7-9][0-9]{9}$/;
		if(regx01.test(mo1))
		{
		console.log("true");
		}
		else{
		alert("Please enter valid mobile number");
		return false;
		}
		
		var  aadhar1= document.getElementById("aadharno1").value;
		var regx03 = /^\d{12}$/;
		if(regx03.test(aadhar1))
		{
		console.log("true");
		}
		else{
		alert("Please enter valid Aadhar number");
		return false;
		}
		
		
		var firstpass1 = document.getElementById("passw01").value;		
		var regx04 = /.{8,20}/;
		if(regx04.test(firstpass1))
		{
		console.log("true");
		}
		else{
		alert("Please enter password of atleast 8 character");
		return false;
		}
		
		var secondpass1 = document.getElementById("pass01").value;
		if(firstpass1==secondpass1)
		{
		return true;
		}
	
		else{
		alert("Both passwords do not match ");
		return false;
		}
		}

				
