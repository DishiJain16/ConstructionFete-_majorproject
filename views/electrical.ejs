<div class="card-body">
<h2 class="card-title text-center" ><%= item.Name%></h2>

<h4 class = "card-text text-center">&nbsp <%=item.youare%></h4>
<h4 class="card-title text-center" ><i class="fa fa-mobile"></i>&nbsp<%= item.mnumber%></h4>
<h4 class="card-title text-center" ><i class="fa fa-envelope"></i>&nbsp<%= item.Email%></h4>
<h4 class = "card-text text-center"><i class="fa fa-address-card"></i>&nbsp <%=item.Saddress%></h4>
<h4 class = "card-text text-center">Range:&nbsp <%=item.cement%></h4>
</div>
</div>
</div>
<%i=i+1;%>
<%if(i%3==0)%>
<br>
<% })%>
